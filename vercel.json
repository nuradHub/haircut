{
  "headers": [
    {
      "source": "/(sitemap.xml|robots.txt)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [],
  "rewrites": [
    {
      "source": "/(sitemap.xml|robots.txt)",
      "destination": "/$1"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
